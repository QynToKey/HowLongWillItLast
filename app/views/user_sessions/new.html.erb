<h1>ログイン</h1>

<%= form_with url: login_path, local: true do |f| %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, nil, autocomplete: "current-password" %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password, nil, autocomplete: "current-password" %>
  </div>

  <div>
    <%= submit_tag "ログイン" %>
  </div>
<% end %>
